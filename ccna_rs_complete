TASK#2 Configuration for Analog 
analog 1
conf t 
  voice-port 0/3/0 
    no shutdown 
    cptone PH/US
    comfort-noise
    signal loopstart 
    caller-id enable 
    station-id name "name1"
    exit
  dial-peer voice 1 pots -->> analog 
    destination-pattern M30 -->> M "monitor number" 
    port 0/3/0
    do csim start M30 -->> to test if its working

analog 2
conf t 
  voice-port 0/3/1 
    no shutdown 
    cptone PH/US
    comfort-noise
    signal loopstart 
    caller-id enable 
    station-id name "name2"
    exit
  dial-peer voice 2 pots -->> analog 
    destination-pattern M31 -->> M "monitor number" 
    port 0/3/1
    do csim start M31 -->> to test if its working

analog 3
conf t 
  voice-port 0/3/2 
    no shutdown 
    cptone PH/US
    comfort-noise
    signal loopstart 
    caller-id enable 
    station-id name "name3"
    exit
  dial-peer voice 3 pots -->> analog
    destination-pattern M32 -->> M "monitor number" 
    port 0/3/2
    do csim start M32 -->> to test if its working

analog 4
conf t 
  voice-port 0/3/3 
    no shutdown 
    cptone PH/US
    comfort-noise
    signal loopstart 
    caller-id enable 
    station-id name "name4"
    exit
  dial-peer voice 4 pots -->> analog 
    destination-pattern M33 -->> M "monitor number" 
    port 0/3/3
    do csim start M33 -->> to test if its working         
     
TASK#3 CISCO HARDWARE "BLADE/SLOT/PORT"
       CISCO PHYSICAL PROPERTIES

TASK#4 MINIMUM CISCO COMMANDS THAT YOU NEED TO KNOW! 
       SUPER BASIC CISCO COMMANDS
conf t 
  hostname VG-NameMo-US/PH 
  enable secret pass -->> put password: pass
  no logging console -->> stfu
  no ip domain-lookup -->> stfu
  no service config -->> stfu
  service pasword-encryption 
  line console 0 -->> light blue cable on console port
    password pass -->> protect console
    login  
    exec-timeout 0 0 -->> disable session timeout (automatic logout)
    exit 
  line vty 0 14 -->> remote persons
    password pass -->> protect remote session: telnet/ssh 
    login 
    exec-timeout 0 0 
    exit 

TASK#5 DHCP SERVER "Dynamic Host Control Protocol"
conf t 
  interface giga 0/0 
    no shutdown 
    ip address 192.168.M.1 255.255.255.0 
    end
ping 192.168.M.1 -->> success rate 100%
conf t 
  ip dhcp excluded-address 192.168.M.1 192.168.M.50 
  ip dhcp pool BIGAYIP 
    network 192.168.M.0 255.255.255.0 
    default-router 192.168.M.1 -->> default gateway
    option 150 ip 192.168.M.1 -->> TFTP of IP Phone
    domain-name CCNA.com 
    dns 8.8.8.8 4.2.2.2 

