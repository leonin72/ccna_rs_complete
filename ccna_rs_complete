TASK#7 
IGP -->> Interior Gateway Protocol 
    -->> inside the company like rip, ospf, eigrp, static
EGP -->> Exterior Gateway Protocol
    -->> outside the company like bgp
conf t
  no router ospf 1
  end

BGP -->> Boarder Gateway Protocol
conf t
  router bgp M
    neighbor 200.M.R.6 remote-as R !!! A,B,C
    neighbor 200.L.M.5 remote-as L !!! B,C,D
    network 200.M.R.4 mask 255.255.255.252 !!! A,B,C
    network 200.L.M.4 mask 255.255.255.252 !!! B,C,D
    network 192.168.M.0 mask 255.255.255.0 !!! A,B,C,D
    no auto-summary
    end
show ip route bgp

